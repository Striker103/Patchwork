<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.control.StartGameViewController">
   <children>
      <BorderPane prefHeight="401.0" prefWidth="602.0">
         <center>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="newGameButton" mnemonicParsing="false" onAction="#onNewGameAction" prefHeight="25.0" prefWidth="85.0" text="New Game">
                     <opaqueInsets>
                        <Insets top="10.0" />
                     </opaqueInsets>
                  </Button>
                  <Button fx:id="loadGameButton" mnemonicParsing="false" onAction="#onLoadGameAction" prefHeight="25.0" prefWidth="85.0" text="Load Game">
                     <opaqueInsets>
                        <Insets top="10.0" />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="highscoresButton" mnemonicParsing="false" onAction="#onHighscoresAction" prefHeight="25.0" prefWidth="85.0" text="Highscores">
                     <opaqueInsets>
                        <Insets top="10.0" />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </center>
      </BorderPane>
      <TextArea prefHeight="200.0" prefWidth="200.0" />
   </children>
</AnchorPane>
